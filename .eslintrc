{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:promise/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "react",
    "promise"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  }
}